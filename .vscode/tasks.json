{
  "version": "2.0.0",
  "tasks": [{
    "label": "server start",
    "command": "docker-compose up",
    "type": "shell",
    "presentation": {
      "reveal": "always",
      "panel": "new"
    },
    "problemMatcher": []
  }, {
    "label": "server stop",
    "command": "docker-compose down",
    "type": "shell",
    "presentation": {
      "reveal": "always",
      "panel": "new"
    },
    "problemMatcher": []
  }, {
    "label": "Run server tests",
    "type": "shell",
    "command": "dotnet",
    "args": [
      "test",
      "${workspaceFolder}/tests/CodePaint.WebApi.Tests.csproj"
    ],
    "group": "test",
    "problemMatcher": "$msCompile"
  }]
}
